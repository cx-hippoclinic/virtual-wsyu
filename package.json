{
  "name": "guet-efm",
  "version": "2.7.17",
  "private": true,
  "description": "桂林电子科技大学企业财务管理虚仿",
  "scripts": {
    "build": "vse-cli build -s both",
    "build:client": "vse-cli build -s client",
    "build:server": "vse-cli build -s server",
    "ci": "npm run build && vse-cli tar",
    "dev:client": "vse-cli dev -s client",
    "dev:server": "vse-cli dev -s server",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "ncu": "ncu -u --target minor --filter @ancademy/*",
    "prepare": "husky install",
    "tsc-w": "tsc -w"
  },
  "prettier": {
    "printWidth": 120,
    "pluginSearchDirs": ["node_modules"],
    "plugins": ["prettier-plugin-organize-imports"]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "extends": [
      "plugin:prettier/recommended"
    ],
    "rules": {
      "prettier/prettier": "error"
    },
    "root": true
  },
  "eslintIgnore": [
    "**/dist",
    "**/build",
    "**/lib",
    "**/libs",
    "**/*.d.ts",
    "modules"
  ],
  "dependencies": {
    "@ancademy/bespoke-robot": "2.9.17",
    "@ancademy/vse-server": "2.9.17",
    "@ancademy/vse-share": "2.9.17",
    "express": "4.18.2"
  },
  "devDependencies": {
    "@ancademy/elf-component": "2.7.7",
    "@ancademy/vse-cli": "2.9.17",
    "@ancademy/vse-client": "2.9.17",
    "@babel/core": "7.20.12",
    "@emotion/css": "11.10.5",
    "@types/express": "4.17.17",
    "@types/node": "18.13.0",
    "@types/react": "18.0.27",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "antd": "5.1.7",
    "eslint": "8.33.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "polished": "4.2.2",
    "prettier": "2.8.4",
    "prettier-plugin-organize-imports": "3.2.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "ts-loader": "9.4.2",
    "typescript": "4.9.5",
    "webpack": "5.75.0"
  }
}
