{
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "module": "commonjs",
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "target": "es6",
    "baseUrl": "src",
    "paths": {
      "@share": ["share"],
      "@client": ["client"],
      "@client/*": ["client/*"],
      "@sub": ["Sub"]
    }
  }
}
